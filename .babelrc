{
  "presets": [
    ["es2015", {
      "modules": false,
      "loose": true
    }],
    "stage-0"
  ],
  "plugins": [
    ["root-import", [{
      "rootPathPrefix": "@",
      "rootPathSuffix": "src"
    }, {
      "rootPathPrefix": "~",
      "rootPathSuffix": "src/lib/util"
    }, {
      "rootPathPrefix": "#",
      "rootPathSuffix": "src/lib/vdom"
    }]],
    ["transform-react-jsx", {
      "pragma": "h" // default pragma is React.createElement
    }],
    "external-helpers"
  ],
  "comments": false
}