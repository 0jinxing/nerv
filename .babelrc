{
  "presets": [
    [
      "es2015",
      {
        "modules": false,
        "loose": true
      }
    ],
    "es2017",
    "stage-0"
  ],
  "plugins": [
    "transform-es3-member-expression-literals",
    "transform-es3-property-literals",
    [
      "transform-react-jsx",
      {
        "pragma": "createElement"
      }
    ]
  ],
  "comments": false
}